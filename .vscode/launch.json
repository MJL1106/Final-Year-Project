    "version": "0.2.0",
    "configurations": [


        {
            "type": "java",
            "name": "Launch Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Launch Driver",
            "request": "launch",
            "mainClass": "com.example.project.Driver",
            "projectName": "project",
            "vmArgs": "--module-path \"project/src/main/resources/com/example/project/javafx-sdk-19/lib\" --add-modules javafx.controls,javafx.fxml"
        }
    ]
}